msgid "User has been created successfully."
msgstr "L'utilisateur a été créé avec succès."

msgid "You have logged in successfully."
msgstr "Vous vous êtes connecté avec succès."

msgid "You have been logged out successfully."
msgstr "Vous vous êtes déconnecté avec succès."

msgid "User already exists."
msgstr "Un utilisateur avec ces informations existe déjà."

msgid "Password is required."
msgstr "Le mot de passe est requis."

msgid "No user found with the provided details."
msgstr "Aucun utilisateur trouvé avec les informations fournies."

msgid "Invalid username or password."
msgstr "Nom d'utilisateur ou mot de passe invalide."

msgid "Your session has expired. Please log in again"
msgstr "Votre session a expiré. Veuillez vous reconnecter."

msgid "Invalid authentication token."
msgstr "Jeton d'authentification invalide."

msgid "The token is invalid or has expired."
msgstr "Le jeton est invalide ou a expiré."

msgid "The password reset link is invalid or has expired."
msgstr "Le lien de réinitialisation du mot de passe est invalide ou a expiré."

msgid "Refresh token not found."
msgstr "Jeton de rafraîchissement introuvable."

msgid "Your session has been refreshed successfully."
msgstr "Votre session a été actualisée avec succès."




msgid "Authentication failed, invalid signature."
msgstr "Échec de l'authentification, signature invalide."

msgid "Authentication failed, payload invalid."
msgstr "Échec de l'authentification, charge utile invalide."

msgid "NOT authenticated."
msgstr "Non authentifié."

msgid "Invalid token type. Expected a refresh token."
msgstr "Type de jeton invalide. Un jeton de rafraîchissement était attendu."

msgid "Invalid token type. Expected an access token."
msgstr "Type de jeton invalide. Un jeton d'accès était attendu."

msgid "Your profile has been updated successfully."
msgstr "Votre profil a été mis à jour avec succès."

msgid "Expense not found."
msgstr "Dépense non trouvée."

msgid "Expense has been removed successfully."
msgstr "La dépense a été supprimée avec succès."
